node('master'){
    stage('Docker'){
        println "hello, world!"
    }

    stage('Grab Artifact'){
        copy()
    }
}

def copy(){
    step([$class: 'CopyArtifact', filter: '*', fingerprintArtifacts: true, projectName: 'multi', selector: [$class: 'StatusBuildSelector', stable: false], target: 'output'])

}